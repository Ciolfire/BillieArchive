{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "4.3.*",
        "doctrine/doctrine-bundle": "3.2.*",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.6.1",
        "league/commonmark": ">=2.8",
        "league/html-to-markdown": ">=5.1.1",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpstan/phpdoc-parser": "^2.3.1",
        "stof/doctrine-extensions-bundle": ">=1.15.2",
        "symfony/asset": ">=8",
        "symfony/asset-mapper": ">=8.0.3",
        "symfony/console": ">=8.0.3",
        "symfony/doctrine-messenger": ">=8.0.3",
        "symfony/dotenv": ">=8",
        "symfony/expression-language": ">=8",
        "symfony/flex": "^2.10",
        "symfony/form": ">=8.0.3",
        "symfony/framework-bundle": ">=8.0.3",
        "symfony/http-client": ">=8.0.3",
        "symfony/intl": ">=8.0.1",
        "symfony/mailer": ">=8.0.3",
        "symfony/mime": ">=8",
        "symfony/monolog-bundle": ">=4.0.1",
        "symfony/notifier": ">=8",
        "symfony/process": ">=8.0.3",
        "symfony/property-access": ">=8.0.3",
        "symfony/property-info": ">=8.0.3",
        "symfony/runtime": ">=8.0.1",
        "symfony/security-bundle": ">=8",
        "symfony/security-csrf": ">=8.0.3",
        "symfony/serializer": ">=8.0.3",
        "symfony/stimulus-bundle": "^2.32",
        "symfony/string": ">=8.0.1",
        "symfony/translation": ">=8.0.3",
        "symfony/twig-bundle": ">=8.0.3",
        "symfony/ux-cropperjs": "^2.32",
        "symfony/ux-dropzone": ">=2.32",
        "symfony/validator": ">=8.0.3",
        "symfony/web-link": ">=8",
        "symfony/yaml": ">=8.0.1",
        "symfonycasts/reset-password-bundle": "^1.24",
        "symfonycasts/verify-email-bundle": ">=1.18",
        "twig/extra-bundle": "^2.12|^3.22.2",
        "twig/intl-extra": ">=3.22.1",
        "twig/markdown-extra": ">=3.22",
        "twig/twig": "^2.12|^3.22.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": ">=8.5.0",
        "doctrine/doctrine-fixtures-bundle": ">=4.3.1",
        "phpunit/phpunit": "^11.5.48",
        "symfony/browser-kit": "8.0.*",
        "symfony/css-selector": "8.0.*",
        "symfony/debug-bundle": "8.0.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/stopwatch": "8.0.*",
        "symfony/web-profiler-bundle": "8.0.*"
    }
}
