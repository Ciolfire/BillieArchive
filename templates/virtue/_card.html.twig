{% extends "_simple_card.html.twig" %}

{% trans_default_domain 'character' %}

{% block data %}
  data-content-name-param="{{ element.name }}"
  data-content-type-param="{{ "#{type}.label.single"|trans }}"
  data-content-link-param="{{ path("#{type}_show", {id: element.id}) }}"
{% endblock %}

{% block title %}
  {{ element.name }}
{% endblock %}

{% block content %}
  {{ element.otherNames }}
{% endblock %}
