
<div id="{{ attribute.identifier }}" class="col-12 col-lg-4 block block-wiki">
  {% if is_granted('ROLE_ST') %}
    <a class="float-end" href="{{ path(entity ~ '_edit', {'id': attribute.id}) }}">{% include "icon.html.twig" with {'icon': 'edit', 'hasTips': 0} only %}</a>
  {% endif %}
  <h3>{{ attribute.name }}</h3>
  <p>{{ attribute.description }}</p>
  <p class="fluff">{{ attribute.fluff }}</p>
</div>
