{% extends "character/sheet/merit/card.html.twig" %}

{% block dot %}
<input id="{{ merit.id ~ '-' ~ value }}" type="checkbox" class="btn-check" autocomplete="off" {% if value <= chMerit.level %}checked{% endif %} disabled>
<label class="btn dot" for="{{ merit.id ~ '-' ~ value }}"></label>
{% endblock %}

{% block expanded %}
<h3>{{ chMerit.choice }}</h3>
{% endblock %}
