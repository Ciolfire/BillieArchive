{% extends "base.html.twig" %}

{% trans_default_domain 'skill' %}

{% block title %}{{ skill.name }}{% endblock %}

{% block body %}
  <div class="container block py-5">
    {% if is_granted('ROLE_ST') %}
      <a class="float-end" href="{{ path('skill_edit', {id: skill.id}) }}">{% include "_icon.html.twig" with {icon: 'edit'} only %}</a>
    {% endif %}
    
    {% include "skill/_show.html.twig" with {isModal: 0} %}
  </div>

  
{% endblock %}
